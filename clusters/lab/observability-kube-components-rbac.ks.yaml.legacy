apiVersion: kustomize.toolkit.fluxcd.io/v1
kind: Kustomization
metadata:
  name: meta-kube-components-rbac
  namespace: flux-system
spec:
  interval: 10m
  path: ./infra/observability/base/kube-components-rbac
  prune: true
  sourceRef:
    kind: GitRepository
    name: gitops
    namespace: flux-system
  dependsOn:
    - name: observability
